{
  "name": "example-angular-15-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "build:dev": "npm run build -- --configuration development",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- --fix",
    "ng": "ng",
    "prepare": "husky",
    "prettier": "prettier --config .prettierrc --write \"**/*.{css,html,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
    "prettier:test": "prettier --config .prettierrc --list-different \"**/*.{css,html,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
    "start": "ng serve",
    "test": "npm run lint && npm run test:ci",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "watch": "ng build --watch --configuration development"
  },
  "lint-staged": {
    "*.{css,html,js,jsx,json,md,scss,ts,tsx,yaml,yml}": [
      "prettier --write"
    ],
    "src/**/*.{html,js,jsx,ts,tsx}": [
      "ng-lint-staged lint --fix --"
    ]
  },
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.11",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-depend": "^0.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jasmine": "^4.2.2",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^15.2.11",
    "ng-lint-staged": "^12.0.4",
    "prettier": "^3.4.2",
    "typescript": "~4.9.4"
  }
}
